<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="padilah.farid.sqlite.sqlitewithactive">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".transaction.AddActivity" />
        <activity android:name=".transaction.ShowActivity" />
        <activity android:name=".transaction.UpdateActivity" />
        <activity android:name=".auth.RegisterActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>